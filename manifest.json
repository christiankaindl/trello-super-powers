{
  "manifest_version": 2,
  "name": "__MSG_extensionName__",
  "version": "0.12.2",

  "description": "__MSG_extensionDescription__",
  "homepage_url": "https://github.com/christiankaindl/trello-super-powers",
  "icons": {
    "16": "assets/logos/logo_16.png",
    "32": "assets/logos/logo_32.png",
  	"64": "assets/logos/logo_64.png",
  	"128": "assets/logos/logo_128.png",
    "256": "assets/logos/logo_256.png"
  },
  "permissions": [
  	"tabs", "storage", "https://trello.com/b/*/", "notifications", "downloads"
  ],
  "background": {
  	"scripts": [
      "browser-polyfill.min.js",
      "papaparse.min.js",
      "background.js"
    ]
  },
  "page_action": {
    "default_icon": {
      "16": "assets/logos/logo_16.png",
      "19": "assets/logos/logo_19.png",
      "32": "assets/logos/logo_32.png",
      "38": "assets/logos/logo_38.png",
      "64": "assets/logos/logo_64.png",
      "128": "assets/logos/logo_128.png",
      "256": "assets/logos/logo_256.png"
    },
  "default_title": "Export Trello board as CSV",
  "default_popup": "/page-action/index.html"
  },

  "options_ui": {
    "page": "settings/options.html"
  },

  "default_locale": "en"
}
